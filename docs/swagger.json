    "host": "quotesmonger.onrender.com",
    "basePath": "/",
    "paths": {
        "/quote/{id}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "get a quote by ID.",
                "operationId": "get-quote-by-id",
                "parameters": [
                    {
                        "type": "string",
                        "description": "quote ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "arrays"
                        }
                    }
                }
            }
        },
        "/quotes": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "return all quotes stored in the database.",
                "operationId": "get-all-quotes",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "arrays"
                        }
                    }
                }
            }
        }
    }
}